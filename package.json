    "@tailwindcss/typography": "^0.5.0",
    "@types/marked": "^4.0.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^6.5.0",
    "prettier": "^2.5.1",
